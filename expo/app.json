{
  "expo": {
    "name": "Smartz",
    "slug": "smartz-expo",
    "version": "1.0.0",

    "sdkVersion": "48.0.0",
    "platforms": ["ios", "android", "web"],

    "orientation": "portrait",
    "icon": "./assets/favicon.png",
    "userInterfaceStyle": "light",

    "splash": {
      "image": "./assets/favicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.smartq.customer"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/favicon.png",
        "backgroundColor": "#3b82f6"
      },
      "package": "com.smartq.customer"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/favicon.png",
          "color": "#3b82f6"
        }
      ]
    ],
    "assetBundlePatterns": ["**/*"]
  }
}